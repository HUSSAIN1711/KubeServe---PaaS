{
  "dashboard": {
    "title": "KubeServe Deployment Dashboard",
    "description": "Per-deployment dashboard showing metrics for a specific model deployment",
    "tags": ["kubeserve", "deployment"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "templating": {
      "list": [
        {
          "name": "namespace",
          "type": "query",
          "label": "Namespace",
          "datasource": "Prometheus",
          "query": "label_values(kube_deployment_status_replicas, namespace)",
          "refresh": 1,
          "regex": "",
          "sort": 1,
          "multi": false,
          "includeAll": false,
          "current": {
            "text": "user-1",
            "value": "user-1"
          }
        },
        {
          "name": "deployment",
          "type": "query",
          "label": "Deployment",
          "datasource": "Prometheus",
          "query": "label_values(kube_deployment_status_replicas{namespace=\"$namespace\"}, deployment)",
          "refresh": 1,
          "regex": "",
          "sort": 1,
          "multi": false,
          "includeAll": false
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{namespace=\"$namespace\",deployment=\"$deployment\"}[5m]))",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 2,
        "title": "Prediction Latency",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(prediction_latency_ms_sum{namespace=\"$namespace\",deployment=\"$deployment\"}[5m]) / rate(prediction_latency_ms_count{namespace=\"$namespace\",deployment=\"$deployment\"}[5m])",
            "legendFormat": "Average",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(prediction_latency_ms_bucket{namespace=\"$namespace\",deployment=\"$deployment\"}[5m]))",
            "legendFormat": "P50",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(prediction_latency_ms_bucket{namespace=\"$namespace\",deployment=\"$deployment\"}[5m]))",
            "legendFormat": "P95",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.99, rate(prediction_latency_ms_bucket{namespace=\"$namespace\",deployment=\"$deployment\"}[5m]))",
            "legendFormat": "P99",
            "refId": "D"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Latency (ms)"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 3,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"$namespace\",pod=~\"$deployment.*\",container=\"inference-server\"}[5m])) by (pod)",
            "legendFormat": "{{pod}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "CPU Usage (cores)"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(container_memory_usage_bytes{namespace=\"$namespace\",pod=~\"$deployment.*\",container=\"inference-server\"}) by (pod)",
            "legendFormat": "{{pod}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Memory Usage"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 5,
        "title": "Pod Replicas",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "kube_deployment_status_replicas{namespace=\"$namespace\",deployment=\"$deployment\"}",
            "legendFormat": "Desired",
            "refId": "A"
          },
          {
            "expr": "kube_deployment_status_replicas_ready{namespace=\"$namespace\",deployment=\"$deployment\"}",
            "legendFormat": "Ready",
            "refId": "B"
          },
          {
            "expr": "kube_deployment_status_replicas_available{namespace=\"$namespace\",deployment=\"$deployment\"}",
            "legendFormat": "Available",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Replicas"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 6,
        "title": "Prediction Success/Error Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "rate(predictions_total{namespace=\"$namespace\",deployment=\"$deployment\",status=\"success\"}[5m])",
            "legendFormat": "Success",
            "refId": "A"
          },
          {
            "expr": "rate(predictions_total{namespace=\"$namespace\",deployment=\"$deployment\",status=\"error\"}[5m])",
            "legendFormat": "Error",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Predictions/sec"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 7,
        "title": "Current Requests/sec",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 24
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{namespace=\"$namespace\",deployment=\"$deployment\"}[1m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 8,
        "title": "Average Latency",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 24
        },
        "targets": [
          {
            "expr": "rate(prediction_latency_ms_sum{namespace=\"$namespace\",deployment=\"$deployment\"}[5m]) / rate(prediction_latency_ms_count{namespace=\"$namespace\",deployment=\"$deployment\"}[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 200
                },
                {
                  "color": "red",
                  "value": 500
                }
              ]
            },
            "unit": "ms"
          }
        }
      },
      {
        "id": 9,
        "title": "Success Rate",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 24
        },
        "targets": [
          {
            "expr": "rate(predictions_total{namespace=\"$namespace\",deployment=\"$deployment\",status=\"success\"}[5m]) / rate(predictions_total{namespace=\"$namespace\",deployment=\"$deployment\"}[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.95
                },
                {
                  "color": "red",
                  "value": 0.90
                }
              ]
            },
            "unit": "percentunit",
            "max": 1,
            "min": 0
          }
        }
      },
      {
        "id": 10,
        "title": "Active Pods",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 24
        },
        "targets": [
          {
            "expr": "kube_deployment_status_replicas_ready{namespace=\"$namespace\",deployment=\"$deployment\"}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "annotations": {
      "list": []
    }
  }
}

