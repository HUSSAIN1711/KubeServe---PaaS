{
  "dashboard": {
    "title": "KubeServe Master Dashboard",
    "description": "Master dashboard showing CPU usage per user, total request rate, and error rates across all deployments",
    "tags": ["kubeserve", "master", "overview"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Total Request Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=~\".*model-serving.*\"}[5m]))",
            "legendFormat": "Total Requests/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=~\".*model-serving.*\",code=~\".*[45].*\"}[5m]))",
            "legendFormat": "Error Rate",
            "refId": "A"
          },
          {
            "expr": "sum(rate(predictions_total{status=\"error\"}[5m]))",
            "legendFormat": "Prediction Errors",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Errors/sec"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 3,
        "title": "CPU Usage per User Namespace",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=~\"user-.*\",container=\"inference-server\"}[5m])) by (namespace)",
            "legendFormat": "{{namespace}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "CPU Usage (cores)"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 4,
        "title": "Memory Usage per User Namespace",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(container_memory_usage_bytes{namespace=~\"user-.*\",container=\"inference-server\"}) by (namespace)",
            "legendFormat": "{{namespace}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Memory Usage"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 5,
        "title": "Average Prediction Latency",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "rate(prediction_latency_ms_sum{job=~\".*model-serving.*\"}[5m]) / rate(prediction_latency_ms_count{job=~\".*model-serving.*\"}[5m])",
            "legendFormat": "Average Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(prediction_latency_ms_bucket{job=~\".*model-serving.*\"}[5m]))",
            "legendFormat": "95th Percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(prediction_latency_ms_bucket{job=~\".*model-serving.*\"}[5m]))",
            "legendFormat": "99th Percentile",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Latency (ms)"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 6,
        "title": "Prediction Success Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "rate(predictions_total{status=\"success\"}[5m]) / rate(predictions_total[5m])",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Success Rate",
            "max": 1,
            "min": 0
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 7,
        "title": "Active Deployments",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 24
        },
        "targets": [
          {
            "expr": "count(kube_deployment_status_replicas{namespace=~\"user-.*\"})",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 8,
        "title": "Total Pods",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 24
        },
        "targets": [
          {
            "expr": "sum(kube_deployment_status_replicas{namespace=~\"user-.*\"})",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 9,
        "title": "Total Predictions",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 24
        },
        "targets": [
          {
            "expr": "sum(increase(predictions_total[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 10,
        "title": "Error Rate Percentage",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 24
        },
        "targets": [
          {
            "expr": "100 * (sum(rate(predictions_total{status=\"error\"}[5m])) / sum(rate(predictions_total[5m])))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            },
            "unit": "percent"
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    }
  }
}

